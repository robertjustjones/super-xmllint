{
  "name": "super-xmlllint",
  "version": "1.0.3",
  "description": "Wrapper for `xmllint`. Easy validation of XML. C14n and c14n-exc canoncalization. XML Schemas and DTDs supported. Zero dependencies.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "nyc mocha",
    "lint": "eslint .",
    "postinstall": "node ./lib/validate-xmllint-installation.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JaanTaponen/super-xmllint"
  },
  "author": "Jaan Taponen <jaan.taponen@aalto.fi>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.9.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "typescript": "^3.7.5",
    "eslint": "^6.8.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0"
  },
  "keywords": [
    "xml",
    "schema",
    "xsd",
    "validation",
    "xmllint",
    "dtd",
    "well-formed",
    "c14n",
    "c14n-exc",
    "canoncalization"
  ]
}
